datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt  DateTime @default(now())
  email      String?
  firstName  String?
  id         String   @id @default(cuid())
  lastName   String?
  middleName String?
  password   String?
  updatedAt  DateTime @updatedAt
  username   String?
}
